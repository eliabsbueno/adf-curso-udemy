{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-curso-udemy"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks'"
		},
		"ls_sql_udemy_curso_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_udemy_curso'"
		},
		"ls_mysql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_mysql'"
		},
		"ls_adls_udemy_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'ls_adls_udemy'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://workshopadls.dfs.core.windows.net"
		},
		"ls_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akv-curso-udemy.vault.azure.net/"
		},
		"ls_ftp_ibge_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ftp.ibge.gov.br/"
		},
		"trg_storage_event_properties_ingest_salesLT_product_parameters_tbName": {
			"type": "string",
			"defaultValue": "Product"
		},
		"trg_storage_event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/629adbd4-fb6e-4c3f-b6a9-11632cbdf973/resourceGroups/workshop/providers/Microsoft.Storage/storageAccounts/workshopadls"
		},
		"ls_adls_udemy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sacursofundamentos.dfs.core.windows.net"
		},
		"ls_adls_udemy_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "3245bc8a-a3a7-40b4-91dd-408347fc3701"
		},
		"ls_adls_udemy_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "9ba758f3-ff10-45b1-8ee5-85be746601ae"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-curso-udemy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_databricks_accessToken": {
						"value": "[parameters('ls_databricks_accessToken')]"
					},
					"ls_adls_udemy_servicePrincipalCredential": {
						"value": "[parameters('ls_adls_udemy_servicePrincipalCredential')]"
					},
					"ls_sql_udemy_curso_connectionString": {
						"value": "[parameters('ls_sql_udemy_curso_connectionString')]"
					},
					"ls_mysql_connectionString": {
						"value": "[parameters('ls_mysql_connectionString')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_akv_properties_typeProperties_baseUrl')]"
					},
					"ls_ftp_ibge_properties_typeProperties_host": {
						"value": "[parameters('ls_ftp_ibge_properties_typeProperties_host')]"
					},
					"ls_adls_udemy_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_udemy_properties_typeProperties_url')]"
					},
					"ls_adls_udemy_properties_typeProperties_tenant": {
						"value": "[parameters('ls_adls_udemy_properties_typeProperties_tenant')]"
					},
					"ls_adls_udemy_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_adls_udemy_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-curso-udemy_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trg_storage_event_properties_ingest_salesLT_product_parameters_tbName": {
						"value": "[parameters('trg_storage_event_properties_ingest_salesLT_product_parameters_tbName')]"
					},
					"trg_storage_event_properties_typeProperties_scope": {
						"value": "[parameters('trg_storage_event_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-curso-udemy_ArmTemplate_0')]"
			]
		}
	]
}